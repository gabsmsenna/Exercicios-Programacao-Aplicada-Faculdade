Algoritmo "semnome"

Var

   medida, calculo: real
funcao area(num: real):real

var
   a: real
inicio
   a <- pi * medida * medida
   retorne a
fimfuncao

Inicio

   escreval("Digite a medida do círculo: ")
   leia(medida)
   calculo <- area(medida)
   escreval("A área do círculo é: ", calculo)

Fimalgoritmo